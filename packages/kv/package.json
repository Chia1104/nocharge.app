{
  "$schema": "https://json.schemastore.org/package",
  "name": "@nocharge/kv",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./clients": {
      "types": "./src/clients.ts",
      "default": "./src/clients.ts"
    },
    "./env": {
      "types": "./src/env.ts",
      "default": "./src/env.ts"
    },
    "./drizzle/cache": {
      "types": "./src/drizzle/cache.ts",
      "default": "./src/drizzle/cache.ts"
    }
  },
  "scripts": {
    "clean": "git clean -xdf .turbo node_modules dist .cache",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "type:check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@keyv/test-suite": "^2.1.2",
    "@nocharge/eslint-config": "workspace:*",
    "@nocharge/tsconfig": "workspace:*",
    "eslint": "catalog:eslint",
    "typescript": "catalog:"
  },
  "dependencies": {
    "@keyv/postgres": "^2.2.1",
    "@keyv/redis": "^5.1.4",
    "@keyv/valkey": "^1.0.11",
    "@nocharge/utils": "workspace:*",
    "@t3-oss/env-core": "catalog:",
    "drizzle-orm": "^0.44.7",
    "keyv": "^5.5.4",
    "zod": "catalog:"
  }
}
